source ~/.profile

export CDPATH=".:~/Worktrees"

[[ $- == *i* ]] || return

set -o vi

[[ -f ~/.local/share/blesh/ble.sh ]] &&
  source ~/.local/share/blesh/ble.sh

alias ls=eza
alias ap=ansible-playbook

eval "$(fzf --bash)"
eval "$(direnv hook bash)"
eval "$(starship init bash)"

[[ ! ${BLE_VERSION-} ]] || ble-attach

# vim: ft=bash

